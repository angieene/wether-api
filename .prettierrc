{
  "singleQuote": true,
  "printWidth": 100,
  "semi": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "trailingComma": "all",
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrder": ["^@nestjs/(.*)$", "", "<THIRD_PARTY_MODULES>", "", "^src/(.*)$", "", "^[.]"],
  "importOrderSeparation": true
}